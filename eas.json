{
  "cli": {
    "version": ">= 16.18.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "node": "20.19.4",
        "env" : {
           "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
        },
        "android": {
        "googleServicesFile": "google-services.json"
      }
      }
    }
  }
}
